{
    "title": "JobSeeker",
    "type": "object",
    "properties": {
        "firstName": {
            "type": "string",
            "description": "The Jobseeker's first name."
        },
        "lastName": {
            "type": "string",
            "description": "The Jobseeker's last name."
        },
        "age": {
            "description": "The JobSeeker age in years.",
            "type": "integer",
            "minimum": 0
        },
        "email": {
            "type": "string",
            "description": "The JobSeeker's email."
        },
        "salary_expectation": {
            "description": "The JobSeeker's annual salary expectations.",
            "type": "number",
            "minimum": 0
        },
        "applied_jobs": {
            "type": "array",
            "items": {
                "type": "number",
                "description": "List of jobs (id's) the JobSeeker has applied for."
            }
        },
        "saved_documents": {
            "type": "array",
            "items": {
                "type": "string",
                "description": "List of document URI's the JobSeeker has stored in S3."
            }
        }
    }
}